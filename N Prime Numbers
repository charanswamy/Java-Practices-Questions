Printing Prime Numbers of N

package cs;
import java.util.*;

public class pravs{
	public static boolean isPrime(int num) {
		if(num<=0) {
			return false;
		}for (int i=2;i<num;i++) {
			if(num%i==0) {
				return false;
			}
		}
		return true;
	}
	public static void main(String[] args) {
		Scanner sc= new Scanner(System.in);
		System.out.println("Enter the Number");
		int num = sc.nextInt();
		for (int i=2;i<num;i++) {
			if(isPrime(i)) {
				System.out.print(i+" ");
			}
		}
	}
}
